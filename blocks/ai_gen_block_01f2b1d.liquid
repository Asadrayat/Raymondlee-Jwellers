{% doc %}
  @prompt
    I need to showcase 7 services with a title and short description, please give me jewelry related icons also , please give me more options for the icons, i like the ones given but i need more, can you make 4 services in a row, i also need to be able to link each service to the page , give me more options of icons, give me icons related to jewelry and  jewelry services, like a money sign, custom design icon, etc
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-services-grid-{{ ai_gen_id }} {
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-services-container-{{ ai_gen_id }} {
    max-width: 1600px;
    margin: 0 auto;
  }

  .ai-services-heading-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 40px;
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
  }

  .ai-services-grid-wrapper-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: {{ block.settings.grid_gap }}px;
  }

  .ai-service-card-{{ ai_gen_id }} {
    background-color: {{ block.settings.card_background }};
    padding: {{ block.settings.card_padding }}px;
    border-radius: {{ block.settings.card_border_radius }}px;
    text-align: center;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    border: {{ block.settings.card_border_width }}px solid {{ block.settings.card_border_color }};
    display: flex;
    flex-direction: column;
    align-items: center;
    text-decoration: none;
    color: inherit;
  }

  .ai-service-card-{{ ai_gen_id }}:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);
  }

  .ai-service-icon-{{ ai_gen_id }} {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    margin-bottom: 20px;
    color: {{ block.settings.icon_color }};
  }

  .ai-service-icon-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    fill: currentColor;
  }

  .ai-service-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: 600;
    margin-bottom: 12px;
    color: {{ block.settings.title_color }};
  }

  .ai-service-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_size }}px;
    line-height: 1.5;
    color: {{ block.settings.description_color }};
  }

  @media screen and (max-width: 989px) {
    .ai-services-grid-wrapper-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-services-grid-wrapper-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }
  }
{% endstyle %}

<div class="ai-services-grid-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-services-container-{{ ai_gen_id }}">
    {% if block.settings.section_heading != blank %}
      <h2 class="ai-services-heading-{{ ai_gen_id }}">{{ block.settings.section_heading }}</h2>
    {% endif %}

    <div class="ai-services-grid-wrapper-{{ ai_gen_id }}">
      {% for i in (1..7) %}
        {% liquid
          assign service_enabled_key = 'service_' | append: i | append: '_enabled'
          assign service_title_key = 'service_' | append: i | append: '_title'
          assign service_description_key = 'service_' | append: i | append: '_description'
          assign service_icon_key = 'service_' | append: i | append: '_icon'
          assign service_link_key = 'service_' | append: i | append: '_link'

          assign service_enabled = block.settings[service_enabled_key]
          assign service_title = block.settings[service_title_key]
          assign service_description = block.settings[service_description_key]
          assign service_icon = block.settings[service_icon_key]
          assign service_link = block.settings[service_link_key]
        %}

        {% if service_enabled %}
          {% if service_link != blank %}
            <a href="{{ service_link }}" class="ai-service-card-{{ ai_gen_id }}">
          {% else %}
            <div class="ai-service-card-{{ ai_gen_id }}">
          {% endif %}
            <div class="ai-service-icon-{{ ai_gen_id }}">
              {% case service_icon %}
                {% when 'custom_design' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M3 17.25V21h3.75L17.81 9.94l-3.75-3.75L3 17.25zM20.71 7.04c.39-.39.39-1.02 0-1.41l-2.34-2.34c-.39-.39-1.02-.39-1.41 0l-1.83 1.83 3.75 3.75 1.83-1.83z"/>
                  </svg>
                {% when 'repair' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M22.7 19l-9.1-9.1c.9-2.3.4-5-1.5-6.9-2-2-5-2.4-7.4-1.3L9 6 6 9 1.6 4.7C.4 7.1.9 10.1 2.9 12.1c1.9 1.9 4.6 2.4 6.9 1.5l9.1 9.1c.4.4 1 .4 1.4 0l2.3-2.3c.5-.4.5-1.1.1-1.4z"/>
                  </svg>
                {% when 'appraisal' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M14 2H6c-1.1 0-2 .9-2 2v16c0 1.1.9 2 2 2h12c1.1 0 2-.9 2-2V8l-6-6zm-2 16H7v-2h5v2zm5-4H7v-2h10v2zm-2-6V3.5L19.5 8H15z"/>
                  </svg>
                {% when 'cleaning' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 3L2 12h3v8h14v-8h3L12 3zm0 2.8L18.5 11H17v7H7v-7H5.5L12 5.8zM12 9l-3 3h2v4h2v-4h2l-3-3z"/>
                  </svg>
                {% when 'resize' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M19 12h-2v3h-3v2h5v-5zM7 9h3V7H5v5h2V9zm14-6H3c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h18c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zm0 16H3V5h18v14z"/>
                  </svg>
                {% when 'engraving' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M3 17v4h4l11-11-4-4L3 17zm3 2H5v-1l8.5-8.5 1 1L6 19zM20.7 5.6l-2.3-2.3c-.4-.4-1-.4-1.4 0l-1.8 1.8 3.7 3.7 1.8-1.8c.4-.4.4-1 0-1.4z"/>
                  </svg>
                {% when 'watch_repair' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M16 2H8l-1 4H5c-1.1 0-2 .9-2 2v8c0 1.1.9 2 2 2h2l1 4h8l1-4h2c1.1 0 2-.9 2-2V8c0-1.1-.9-2-2-2h-2l-1-4zm-4 18c-3.3 0-6-2.7-6-6s2.7-6 6-6 6 2.7 6 6-2.7 6-6 6zm.5-13H11v5.2l4.5 2.7.8-1.2-4-2.4V7z"/>
                  </svg>
                {% when 'consultation' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 2C6.5 2 2 6.5 2 12s4.5 10 10 10 10-4.5 10-10S17.5 2 12 2zm1 15h-2v-6h2v6zm0-8h-2V7h2v2z"/>
                  </svg>
                {% when 'gift_wrap' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M20 6h-2.2c.4-.5.7-1.2.7-2 0-1.7-1.3-3-3-3-1.4 0-2.5.9-2.9 2.1L12 4.8l-.6-1.7C11 1.9 9.9 1 8.5 1c-1.7 0-3 1.3-3 3 0 .8.3 1.5.7 2H4c-1.1 0-2 .9-2 2v3h20V8c0-1.1-.9-2-2-2zM8.5 3c.6 0 1 .4 1 1s-.4 1-1 1-1-.4-1-1 .4-1 1-1zm7 0c.6 0 1 .4 1 1s-.4 1-1 1-1-.4-1-1 .4-1 1-1zM2 20c0 1.1.9 2 2 2h7V12H2v8zm11 2h7c1.1 0 2-.9 2-2v-8h-9v10z"/>
                  </svg>
                {% when 'insurance' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 1L3 5v6c0 5.5 3.8 10.7 9 12 5.2-1.3 9-6.5 9-12V5l-9-4zm0 10.9l-3.5 3.5-1.4-1.4L10.6 11 7.1 7.5l1.4-1.4L12 9.6l3.5-3.5 1.4 1.4L13.4 11l3.5 3.5-1.4 1.4L12 11.9z"/>
                  </svg>
                {% when 'restoration' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 6v3l4-4-4-4v3c-4.4 0-8 3.6-8 8 0 1.6.5 3.1 1.3 4.3l1.5-1.5C6.3 14.2 6 13.1 6 12c0-3.3 2.7-6 6-6zm6.7 2.7l-1.5 1.5c.5 1.1.8 2.2.8 3.8 0 3.3-2.7 6-6 6v-3l-4 4 4 4v-3c4.4 0 8-3.6 8-8 0-1.6-.5-3.1-1.3-4.3z"/>
                  </svg>
                {% when 'trade_in' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M6.99 11L3 15l3.99 4v-3H14v-2H6.99v-3zM21 9l-3.99-4v3H10v2h7.01v3L21 9z"/>
                  </svg>
                {% when 'financing' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M11.8 10.9c-2.3-.5-3-.9-3-1.9 0-1.1 1.1-1.8 2.8-1.8 1.8 0 2.5.8 2.6 2h2.3c-.1-1.7-1.1-3.2-3.1-3.7V3h-3v2.5c-1.9.4-3.4 1.6-3.4 3.6 0 2.3 1.9 3.4 4.6 4 2.5.5 3 1.3 3 2.2 0 .7-.5 1.8-2.8 1.8-2.1 0-3-.9-3.1-2H4.3c.1 2.2 1.6 3.4 3.7 3.9V21h3v-2.5c1.9-.4 3.4-1.5 3.4-3.6 0-2.9-2.4-3.6-4.6-4z"/>
                  </svg>
                {% when 'layaway' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M17 2H7c-1.1 0-2 .9-2 2v16c0 1.1.9 2 2 2h10c1.1 0 2-.9 2-2V4c0-1.1-.9-2-2-2zm0 18H7V4h10v16zM9 6h6v2H9V6zm0 4h6v2H9v-2zm0 4h6v2H9v-2z"/>
                  </svg>
                {% when 'warranty' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 1L3 5v6c0 5.5 3.8 10.7 9 12 5.2-1.3 9-6.5 9-12V5l-9-4zm-2 16l-4-4 1.4-1.4L10 14.2l6.6-6.6L18 9l-8 8z"/>
                  </svg>
                {% when 'delivery' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M18 18.5c-.8 0-1.5.7-1.5 1.5s.7 1.5 1.5 1.5 1.5-.7 1.5-1.5-.7-1.5-1.5-1.5zM19.5 9.5h-1V6c0-.6-.4-1-1-1h-11c-.6 0-1 .4-1 1v9c0 .6.4 1 1 1h.8c.3-1.3 1.5-2.3 2.9-2.3s2.6 1 2.9 2.3h3.3c.3-1.3 1.5-2.3 2.9-2.3s2.6 1 2.9 2.3h.8c.6 0 1-.4 1-1v-3.5l-3.5-3.5zm-1 1.5l2.5 2.5H17v-2.5h1.5zM10.2 18.5c-.8 0-1.5.7-1.5 1.5s.7 1.5 1.5 1.5 1.5-.7 1.5-1.5-.7-1.5-1.5-1.5z"/>
                  </svg>
                {% when 'authentication' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 1L3 5v6c0 5.5 3.8 10.7 9 12 5.2-1.3 9-6.5 9-12V5l-9-4zm7 10c0 4.5-3.2 8.7-7 9.9-3.8-1.2-7-5.4-7-9.9V6.3l7-3.1 7 3.1V11zm-9 .4L8.6 10 7.2 11.4l2.8 2.8 5.7-5.7-1.4-1.4-4.3 4.3z"/>
                  </svg>
                {% when 'gift_registry' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 5.9c1.2 0 2.1-.9 2.1-2.1 0-1.2-.9-2.1-2.1-2.1-1.2 0-2.1.9-2.1 2.1 0 1.2.9 2.1 2.1 2.1zm-2 .1c-.6 0-1.1.5-1.1 1.1V9h-2v11c0 1.1.9 2 2 2h6c1.1 0 2-.9 2-2V9h-2V7.1c0-.6-.5-1.1-1.1-1.1H10zm1 5h2v8h-2v-8z"/>
                  </svg>
                {% when 'personalization' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 12c2.2 0 4-1.8 4-4s-1.8-4-4-4-4 1.8-4 4 1.8 4 4 4zm0 2c-2.7 0-8 1.3-8 4v2h16v-2c0-2.7-5.3-4-8-4z"/>
                  </svg>
                {% when 'polishing' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 1l2.5 7.5L22 11l-7.5 2.5L12 21l-2.5-7.5L2 11l7.5-2.5L12 1zm0 4.5L10.5 10 6 11.5l4.5 1.5L12 17.5l1.5-4.5L18 11.5 13.5 10 12 5.5z"/>
                  </svg>
                {% when 'setting' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 2L4 9l8 13 8-13-8-7zm0 2.5L17.5 9h-11L12 4.5zM6.5 10h11L12 19.5 6.5 10z"/>
                  </svg>
                {% when 'remounting' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 2L9 8H6l-2 3c0 5.5 3.6 10 8 10s8-4.5 8-10l-2-3h-3l-3-6zm0 3l1.5 3h-3L12 5zm-5 6h10c0 3.9-2.2 7-5 7s-5-3.1-5-7z"/>
                  </svg>
                {% when 'valuation' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M5 9.2h3V19H5V9.2zM10.6 5h2.8v14h-2.8V5zm5.6 8H19v6h-2.8v-6z"/>
                  </svg>
                {% when 'inspection' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M15.5 14h-.8l-.3-.3c1-1.1 1.6-2.6 1.6-4.2C16 5.9 13.1 3 9.5 3S3 5.9 3 9.5 5.9 16 9.5 16c1.6 0 3.1-.6 4.2-1.6l.3.3v.8l5 5 1.5-1.5-5-5zm-6 0C7 14 5 12 5 9.5S7 5 9.5 5 14 7 14 9.5 12 14 9.5 14z"/>
                  </svg>
                {% when 'lifetime_care' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 21.4L10.6 20C5.4 15.4 2 12.3 2 8.5 2 5.4 4.4 3 7.5 3c1.7 0 3.4.8 4.5 2.1C13.1 3.8 14.8 3 16.5 3 19.6 3 22 5.4 22 8.5c0 3.8-3.4 6.9-8.6 11.5L12 21.4z"/>
                  </svg>
                {% when 'upgrade' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M16 18l2.3-2.3-5.3-5.3-5.3 5.3L10 18l2-2 2 2zm-2-5l5.3-5.3L17 5.7l-5 5-5-5L4.7 7.7 10 13l2-2 2 2z"/>
                  </svg>
                {% when 'buyback' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 2C6.5 2 2 6.5 2 12s4.5 10 10 10 10-4.5 10-10S17.5 2 12 2zm1 14.9c-2.3-.5-3-.9-3-1.9 0-1.1 1.1-1.8 2.8-1.8 1.8 0 2.5.8 2.6 2h2.3c-.1-1.7-1.1-3.2-3.1-3.7V9h-3v2.5c-1.9.4-3.4 1.6-3.4 3.6 0 2.3 1.9 3.4 4.6 4 2.5.5 3 1.3 3 2.2 0 .7-.5 1.8-2.8 1.8-2.1 0-3-.9-3.1-2H6.3c.1 2.2 1.6 3.4 3.7 3.9V27h3v-2.5c1.9-.4 3.4-1.5 3.4-3.6 0-2.9-2.4-3.6-4.6-4z"/>
                  </svg>
                {% when 'free_shipping' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M20 8h-3V4H3c-1.1 0-2 .9-2 2v11h2c0 1.7 1.3 3 3 3s3-1.3 3-3h6c0 1.7 1.3 3 3 3s3-1.3 3-3h2v-5l-3-4zM6 18.5c-.8 0-1.5-.7-1.5-1.5s.7-1.5 1.5-1.5 1.5.7 1.5 1.5-.7 1.5-1.5 1.5zm13.5-9l1.9 2.5H17V9.5h2.5zM18 18.5c-.8 0-1.5-.7-1.5-1.5s.7-1.5 1.5-1.5 1.5.7 1.5 1.5-.7 1.5-1.5 1.5z"/>
                  </svg>
                {% when 'appointment' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M19 3h-1V1h-2v2H8V1H6v2H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zm0 16H5V8h14v11zM7 10h5v5H7z"/>
                  </svg>
                {% when 'virtual_try_on' %}
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                    <path d="M12 4.5C7 4.5 2.7 7.6 1 12c1.7 4.4 6 7.5 11 7.5s9.3-3.1 11-7.5c-1.7-4.4-6-7.5-11-7.5zM12 17c-2.8 0-5-2.2-5-5s2.2-5 5-5 5 2.2 5 5-2.2 5-5 5zm0-8c-1.7 0-3 1.3-3 3s1.3 3 3 3 3-1.3 3-3-1.3-3-3-3z"/>
                  </svg>
              {% endcase %}
            </div>
            <h3 class="ai-service-title-{{ ai_gen_id }}">{{ service_title }}</h3>
            <p class="ai-service-description-{{ ai_gen_id }}">{{ service_description }}</p>
          {% if service_link != blank %}
            </a>
          {% else %}
            </div>
          {% endif %}
        {% endif %}
      {% endfor %}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Jewelry services",
  "settings": [
    {
      "type": "header",
      "content": "Section"
    },
    {
      "type": "text",
      "id": "section_heading",
      "label": "Section heading",
      "default": "Our Services"
    },
    {
      "type": "range",
      "id": "section_padding",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Section padding",
      "default": 50
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background color",
      "default": "#ffffff"
    },
    {
      "type": "header",
      "content": "Heading style"
    },
    {
      "type": "range",
      "id": "heading_size",
      "min": 20,
      "max": 60,
      "step": 2,
      "unit": "px",
      "label": "Heading size",
      "default": 36
    },
    {
      "type": "color",
      "id": "heading_color",
      "label": "Heading color",
      "default": "#000000"
    },
    {
      "type": "header",
      "content": "Grid layout"
    },
    {
      "type": "range",
      "id": "grid_gap",
      "min": 10,
      "max": 50,
      "step": 5,
      "unit": "px",
      "label": "Gap between cards",
      "default": 30
    },
    {
      "type": "header",
      "content": "Card style"
    },
    {
      "type": "color",
      "id": "card_background",
      "label": "Card background",
      "default": "#f9f9f9"
    },
    {
      "type": "range",
      "id": "card_padding",
      "min": 10,
      "max": 60,
      "step": 5,
      "unit": "px",
      "label": "Card padding",
      "default": 30
    },
    {
      "type": "range",
      "id": "card_border_radius",
      "min": 0,
      "max": 30,
      "step": 2,
      "unit": "px",
      "label": "Card border radius",
      "default": 8
    },
    {
      "type": "range",
      "id": "card_border_width",
      "min": 0,
      "max": 5,
      "step": 1,
      "unit": "px",
      "label": "Card border width",
      "default": 1
    },
    {
      "type": "color",
      "id": "card_border_color",
      "label": "Card border color",
      "default": "#e0e0e0"
    },
    {
      "type": "header",
      "content": "Icon style"
    },
    {
      "type": "range",
      "id": "icon_size",
      "min": 30,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Icon size",
      "default": 60
    },
    {
      "type": "color",
      "id": "icon_color",
      "label": "Icon color",
      "default": "#aa7248"
    },
    {
      "type": "header",
      "content": "Text style"
    },
    {
